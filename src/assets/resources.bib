  shorttitle   = {The User in the Loop},
  author       = {Evers, Christoph and Kniewel, Romy and Geihs, Kurt and Schmidt, Ludger},
  date         = {2014-05-01},
  journaltitle = {Future Generation Computer Systems},
  series       = {Special {{Section}}: {{Distributed Solutions}} for {{Ubiquitous Computing}} and {{Ambient Intelligence}}},
